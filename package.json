{
    "name": "juniper-lang",
    "displayName": "Juniper Lang",
    "description": "Syntax highlighting for Juniper lang",
    "version": "1.0.0",
    "publisher": "calebh",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/calebh/juniper-lang.git"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "juniper",
                "aliases": [
                    "Juniper",
                    "juniper"
                ],
                "extensions": [
                    ".jun"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "juniper",
                "scopeName": "source.juniper",
                "path": "./syntaxes/juniper.tmLanguage.json"
            }
        ]
    }
}
